{
    "name": "endroid/pusher-bundle",
    "description": "Endroid Pusher Bundle for Symfony",
    "keywords": ["endroid", "pusher", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/pusher-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "https://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "pusher/pusher-php-server": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\PusherBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\PusherBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
