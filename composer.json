{
    "name": "endroid/pusher-bundle",
    "description": "Endroid Pusher Bundle for Symfony",
    "keywords": ["endroid", "pusher", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidPusherBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "pusher/pusher-php-server": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\PusherBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Bundle\\PusherBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
